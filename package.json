{
  "name": "@netilon/differify",
  "version": "3.0.0",
  "description": "Differify allows you to get the difference between two entities (objects, arrays, dates, functions, numbers, etc) very easy, quickly and in a friendly way.",
  "main": "./index.js",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "start:dev": "NODE_ENV=development nodemon index.js",
    "debug": "node --inspect-brk=9229 --nolazy index.js",
    "build": "rm -rf ./index.js && npx webpack",
    "lint": "eslint ./src",
    "test": "jest",
    "coverage": "jest --coverage",
    "benchmark": "node ./test/differify.benchmark.js"
  },
  "keywords": [
    "differify",
    "diff",
    "compare",
    "comparator",
    "comparison",
    "react",
    "angular",
    "vue",
    "browser",
    "netilon"
  ],
  "author": "Fabian Roberto Orue <fabianorue@gmail.com> (https://www.netilon.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/netilon/differify"
  },
  "bugs": {
    "url": "https://github.com/netilon/differify/issues",
    "email": "fabianorue@gmail.com"
  },
  "homepage": "http://www.netilon.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.1.0",
    "benchmark": "^2.1.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^26.0.1",
    "nodemon": "^2.0.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
